<!doctype html>
<html lang="en">
	{{>head}}
	<link href="css/blog.css" rel="stylesheet">
  	</head>

	<body>

		{{>header}}

		<div class="container">
			<div class="row">
				<div class="col-md-3 col-sm-12 sidebar">
					<h3>{{page.sidebar.news.title}}</h3>
					<ul class="news-list">
						{{#page.sidebar.news.list}}
							<li>
								<img src={{{img}}}>
								<p><a href="#">{{text}}</a></p>
							</li>
						{{/page.sidebar.news.list}}
					</ul>
					<h3>{{page.sidebar.tags.title}}</h3>
					<ul class="tag-list">
						{{#page.sidebar.tags.list}}
							<li>
								<a href="#">{{text}}</a>
							</li>
						{{/page.sidebar.tags.list}}
				</div>
				<div class="col-md-9 col-sm-12">
				</div>
			</div>
		</div>
		{{>footer}}

		<script src="js/jquery.min.js"></script>
		<script src="js/bootstrap.min.js"></script>
		<script src="js/custom.js"></script>

	</body>
</html>